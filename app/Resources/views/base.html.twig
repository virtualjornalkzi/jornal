                integrity="sha384-vBWWzlZJ8ea9aCX4pEW3rVHjgjt7zpkNpZk+02D9phzyeVkE+jo0ieGizqPLForn"
                crossorigin="anonymous"></script>
        <script src="{{ asset('public/js/jQuery plugins/animate.js') }}"></script>
        <script src="{{ asset('public/js/supporting.js') }}"></script>
        <script src="{{ asset('public/js/base.js') }}"></script>
        <script src="{{ asset('public/js/header.js') }}"></script>
        <script src="{{ asset('public/js/notifications.js') }}"></script>
        {% block javascripts %}{% endblock %}
        {% block stylesheets %}{% endblock %}
    </head>
    <body>
        {% block header %}{% include 'default/header.html.twig' %}{% endblock %}
        <div class="content container-fluid">
            {% block content %}{% endblock %}
        </div>
        {% block footer %}{% include 'default/footer.html.twig' %}{% endblock %}
    </body>
</html>
