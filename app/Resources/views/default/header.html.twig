<div class="header">
    <nav class="navbar navbar-default" role="navigation">
        <div class="container-fluid">
            <!-- Mobile display menu -->
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#header-navbar">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
            </div>

            <!-- Collect the nav links, forms, and other content for toggling -->
            <div class="collapse navbar-collapse" id="header-navbar">
                <ul class="nav navbar-nav">
                    <a class="header-emblem navbar-brand" href="#">
                        {% block headeremblem %}
                            <img src="{{ asset('static/img/temp-logo.png') }}" alt="header emblem" class="header-emblem-img img-circle">
                        {% endblock %}
                    </a>
                    <!-- Menu -->
                    {% block headermenu %}
                        <li class="header-menu-element"><a class="header-menu-element-link" href="#">Link 1<span class="sr-only">(current)</span></a></li>
                        <li class="header-menu-element"><a class="header-menu-element-link" href="#">Link</a></li>
                        <li class="header-menu-element dropdown">
                            <a href="#" class="header-menu-element-link dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">Dropdown <span class="caret"></span></a>
                            <ul class="dropdown-menu">
                                <li><a class="header-menu-element-dropdown-link" href="#">Action</a></li>
                                <li><a class="header-menu-element-dropdown-link" href="#">Another action</a></li>
                                <li><a class="header-menu-element-dropdown-link" href="#">Something else here</a></li>
                                <li role="separator" class="divider"></li>
                                <li><a class="header-menu-element-dropdown-link" href="#">Separated link</a></li>
                                <li role="separator" class="divider"></li>
                                <li><a class="header-menu-element-dropdown-link" href="#">One more separated link</a></li>
                            </ul>
                        </li>
                    {% endblock %}
                    <!-- User Profile -->
                    {% block headeruserprofile %}

                        <li class="dropdow pull-right">
                            <a href="#" class="dropdown-toggle header-menu-element-link" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">
                                    <span class="glyphicon glyphicon-menu-hamburger"></span>
                            </a>
                            <ul class="dropdown-menu">
                                <li><a class="header-menu-element-dropdown-link" href="#">Action</a></li>
                                <li><a class="header-menu-element-dropdown-link" href="#">Another action</a></li>
                                <li><a class="header-menu-element-dropdown-link" href="#">Something else here</a></li>
                                <li role="separator" class="divider"></li>
                                <li><a class="header-menu-element-dropdown-link" href="#">Separated link</a></li>
                            </ul>
                        </li>
                        <a class="header-emblem navbar-brand pull-right" href="#">
                            {% block headeremblem2 %}
                                <img src="{{ asset('static/img/temp-logo.png') }}" alt="header emblem" class="header-emblem-img img-circle">
                            {% endblock %}
                        </a>
                    {% endblock %}
                </ul>
            </div><!-- /.navbar-collapse -->
        </div><!-- /.container-fluid -->
    </nav>
</div>
